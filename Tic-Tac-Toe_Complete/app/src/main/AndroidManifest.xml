<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tic_tac_toe" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_x_o"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_x_o"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe" >
        <activity
            android:name=".StartMenu"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".IconSelect"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.IconSelect" />
        </activity>

        <activity
            android:name=".EndScreen"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.EndScreen" />
        </activity>

        <activity
            android:name=".WinScreen"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.WinScreen" />
        </activity>

        <activity
            android:name=".TicTacToe"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.TicTacToe" />
        </activity>

        <activity
            android:name=".TicTacToe2"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.TicTacToe" />
        </activity>

        <activity
            android:name=".TicTacToe3"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.TicTacToe" />
        </activity>

        <activity
            android:name=".TicTacToe4"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.TicTacToe" />
        </activity>

        <activity
            android:name=".NameSelect"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.NameSelect" />
        </activity>

        <activity
            android:name=".NameSelect2"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.NameSelect" />
        </activity>

        <activity
            android:name=".NameSelect3"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.NameSelect" />
        </activity>

        <activity
            android:name=".NameSelect4"
            android:parentActivityName=".StartMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tic_tac_toe.NameSelect" />
        </activity>

    </application>

</manifest>